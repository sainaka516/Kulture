{
  "name": "kulture",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:backup": "bash scripts/backup-db.sh",
    "db:verify-migration": "tsx scripts/verify-migration.ts",
    "db:safe-migrate": "npm run db:backup && npm run db:verify-migration && npx prisma migrate deploy",
    "db:stats": "tsx scripts/verify-migration.ts --stats-only",
    "seed:test": "npx tsx scripts/seed-test-data.ts",
    "update-hierarchy": "tsx scripts/update-hierarchy.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
